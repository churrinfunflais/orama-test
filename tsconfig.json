{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "module": "ES2022",
    "rootDir": "src",
    "outDir": "build",
    "moduleResolution": "node",
    "lib": [
      "ES2022",
      "dom"
    ],
    "target": "ES2022",
    "esModuleInterop": true,
    "skipLibCheck": true,
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts"
  ],
  "exclude": [
    "coverage",
    "node_modules",
    "dist",
    "tests",
    "build"
  ]
}